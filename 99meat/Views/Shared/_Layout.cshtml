<!DOCTYPE html>
<html lang="en" dir="ltr">
<head>
    <meta charset="UTF-8">
    <title>Biryani City</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="format-detection" content="telephone=no">
    <meta name="msapplication-tap-highlight" content="no">

    <link rel="icon" type="image/x-icon" href="assets/img/appicon.png">
    <link rel="manifest" href="~/manifest.json">
    <meta name="theme-color" content="#387ef5">

    <meta name="apple-mobile-web-app-capable" content="yes">
    <link rel="apple-touch-icon" href="assets/img/appicon.png">
    <meta name="theme-color" content="#4e8ef7">
    <script>
    if ('serviceWorker' in navigator) {
      navigator.serviceWorker.register('../service-worker.js')
        .then(() => console.log('service worker installed'))
        .catch(err => console.log('Error', err));
    }
    </script>
    <link href="http://biryanicity.azurewebsites.net/build/main.css" rel="stylesheet">
    <script src="http://maps.google.com/maps/api/js"></script>
    <script src="~/jquery.js"></script>


</head>
<body>
    <!-- Ionic's root component and where the app will load -->
    <ion-app></ion-app>
    <!-- import polyfill -->
    <script src="https://cdn.polyfill.io/v2/polyfill.min.js?features=Intl.~locale.en"></script>
   
    <!-- The polyfills js is generated during the build process -->
    <script src="http://biryanicity.azurewebsites.net/build/polyfills.js"></script>
    <!-- The bundle js is generated during the build process -->
    <script src="http://biryanicity.azurewebsites.net/build/main.js"></script>
    <div class="container body-content">


        @RenderBody()
    </div>

</body>
</html>
